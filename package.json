{
  "name": "coding-agent",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:api": "pnpm --filter api dev",
    "dev:web": "pnpm --filter web dev",
    "worker": "pnpm --filter api worker",
    "db:migrate": "pnpm --filter api db:migrate"
  }
}
